    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "babel-plugin-root-import": "^6.4.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0"
  }
